{
  "expo": {
    "name": "icCam",
    "slug": "icCam",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "$(PRODUCT_NAME) needs access to your Camera."
        }
      ]
    ],
    "android": {
      "permissions": [
        "android.permission.CAMERA"
      ],
      "package": "com.chenmoskovitz.icCam"
    }
  },
  "name": "icCam"
}
